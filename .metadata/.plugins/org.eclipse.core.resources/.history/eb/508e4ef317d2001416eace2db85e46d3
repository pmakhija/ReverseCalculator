package main.java;

import java.util.HashMap;

public class OperatorFactory {

	public static HashMap<String, Object> BinaryOperations;
	public static HashMap<String, Object> UnaryOperations;
	
	static
	{
		new BinaryOperations("+",AdditionStrategy);
	}
	
	
	public HashMap<String, Object> getBinaryOperations() {
		return BinaryOperations;
	}
	public void setBinaryOperations(String operator) {
		BinaryOperations = new BinaryOperations<operator,>;
	}
	public HashMap<String, Object> getUnaryOperations() {
		return UnaryOperations;
	}
	public void setUnaryOperations(HashMap<String, Object> unaryOperations) {
		UnaryOperations = unaryOperations;
	}
	
	
	
	
}
