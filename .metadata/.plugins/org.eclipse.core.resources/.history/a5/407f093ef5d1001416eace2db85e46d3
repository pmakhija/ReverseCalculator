package main.java;

import java.util.Stack;

public class RPNPostFix {
	
	public String sum(String[] arr){
		Stack<int[]> inputStack[];
		int sum=0;
		for(int i=0; i<arr.length; i++){
			if(arr[i].equals("+"))
			{
				int rightVariable = inputStack[inputStack.length];
				int leftVariable = inputStack[inputStack.length-1];
				sum = leftVariable + rightVariable;
				
			}
			else{
				inputStack
			}
		}
		return sum;
	}

}
